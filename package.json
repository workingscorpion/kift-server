  "scripts": {
    "build": "npx tsc",
    "start": "npm run start:dev",
    "start:dev": "better-npm-run start:dev",
    "start:prod": "better-npm-run start:prod",
    "stop": "pkill --signal SIGINT ts-api-server",
    "test": "better-npm-run test",
    "watch": "npm run watch:dev",
    "watch:dev": "better-npm-run watch:dev",
    "watch:prod": "better-npm-run watch:prod"
  },
  "betterScripts": {
    "start:dev": "node $npm_package_main",
    "start:prod": {
      "command": "node $npm_package_main",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "npx jest",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "watch:dev": "nodemon $npm_package_main",
    "watch:prod": {
      "command": "nodemon $npm_package_main",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
